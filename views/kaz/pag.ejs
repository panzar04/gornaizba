
<% function pag(obecnyRok){ %>

<% const data = new Date().getFullYear() + '';const rokTeraz = data.slice(2, 4);%> 

<% function previous (){ %>
    <% if(obecnyRok - 3 > 13){ %>
        <li class="page-item">
            <a class="page-link" href="/nauczanie/20<%= obecnyRok -3 %>" aria-label="Previous">
              <span aria-hidden="true">&laquo;</span>
            </a>
        </li>
        <% } else{ %>
            <li class="page-item disabled">
                <a class="page-link" href="#" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
            </a>
        </li>
    <% }}; %>
<% function minusTwo (){ %>
    <% const a = obecnyRok -2; %>
<% if(a > 13){ %>
    <li class="page-item"><a class="page-link" href="/nauczanie/20<%= a %>"><%= `20${a}` %></a></li>
    <% } %>
<% }; %>
<% function minusOne(){ %>
    <% const b = obecnyRok-1; %>
    <% if(b > 13){ %>
    <li class="page-item"><a class="page-link" href="/nauczanie/20<%= b %>"><%= `20${b}` %></a></li>
    <% } %>
<% }; %>
<% function next (){ %>
    <% const n = obecnyRok + 3; %>
    <% if(n > rokTeraz ){ %>
        
        <li class="page-item disabled">
                <a class="page-link" href="#" aria-label="Next">
                <span aria-hidden="true">&raquo;</span>
            </a>
        </li>
        <% } else{ %>
        <li class="page-item">
            <a class="page-link" href="/nauczanie/20<%= n %>" aria-label="Next">
              <span aria-hidden="true">&raquo;</span>
            </a>
        </li>
    <% }}; %>
<% function plusTwo (){ %>
    <% const c = obecnyRok + 2; %>
<% if(obecnyRok + 2 < rokTeraz){ %>
    <li class="page-item"><a class="page-link" href="/nauczanie/20<%= c %>"><%= `20${c}` %></a></li>
    <% } %>
<% }; %>
<% function plusOne(){ %>
    <% const d = obecnyRok +1; %>
    <% if(obecnyRok + 1 < rokTeraz){ %>
    <li class="page-item"><a class="page-link" href="/nauczanie/20<%= d %>"><%= `20${d}` %></a></li>
    <% }else{ %>
    <li class="page-item d-none"><a class="page-link" href="#"><%= `20${d}` %></a></li> 
<% }}; %>
    
    
    




<!-- <nav aria-label="Page navigation example">
    <ul class="pagination">
      
        <%= previous() %>
        <%= minusTwo() %>
        <%= minusOne() %>
        <li class="page-item disabled"><a class="page-link" href="#"><%= `20${obecnyRok}` %></a></li>
        <%= plusOne() %>
        <%= plusTwo() %>
        <%= next() %>

    </ul>
  </nav> -->
<% } %>